  <joint name="head_tail" type="revolute">
    <axis xyz="0 1 0"/>
    <parent link="head"/>
    <child link="tail"/>
    <origin xyz="-0.5 0.0 0.05"/>
    <limit effort="1000" lower="-3.14" upper="3.14" velocity="1.5"/>
  </joint>
