<?xml version="1.0"?>
<robot name="physics">

  <link name="head">
    <visual>
      <geometry>
        <box size="1 0.5 0.1"/>
      </geometry>
      <origin xyz="0 0 0.05"/>
      <material name="gray">
        <color rgba="0.6 0.6 0.6 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="1 0.5 0.1"/>
      </geometry>
      <origin xyz="0 0 0.05"/>
    </collision>
    <inertial>
      <mass value="1"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
  </link>

  <link name="tail">
    <visual>
      <geometry>
          <box size="1 0.2 0.1"/>
      </geometry>
      <origin xyz="-0.5 0 0.0"/>
      <material name="gray">
        <color rgba="0.6 0.6 0.6 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
          <box size="1 0.2 0.1"/>
      </geometry>
      <origin xyz="-0.5 0 0.0"/>
    </collision>
    <inertial>
      <mass value="1"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
  </link>
